{
    "name": "create-go-app",
    "version": "0.1.16",
    "keywords": [
        "go"
    ],
    "description": "Create Go apps with no build configuration.",
    "repository": {
        "type": "git",
        "url": "https://github.com/gostartkit/create-go-app.git",
        "directory": "packages/create-go-app"
    },
    "author": "GoStartKit team <sam@gostartkit.com>",
    "license": "AGPL",
    "engines": {
        "node": ">=14"
    },
    "bugs": {
        "url": "https://github.com/gostartkit/create-go-app/issues"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "bin": {
        "create-go-app": "./dist/index.js"
    },
    "scripts": {
        "dev": "ncc build ./src/index.ts -w -o dist/",
        "build": "pnpm release",
        "release": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
        "publish": "rm -rf ./dist && pnpm release && npm publish",
        "start": "ts-node src/index.ts"
    },
    "devDependencies": {
        "@types/node": "^20.14.2",
        "@types/prompts": "^2.4.9",
        "@vercel/ncc": "^0.38.1",
        "commander": "^12.1.0",
        "picocolors": "^1.0.1",
        "prompts": "^2.4.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "update-check": "^1.5.4"
    }
}